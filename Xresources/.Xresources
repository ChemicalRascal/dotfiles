!! urxvt settings.

!Globals
URxvt.scrollBar:	false
URxvt.loginShell:	true
URxvt*secondaryScroll:	true
URxvt*saveLines:	65535
URxvt*urgentOnBell:	true
URxvt*internalBorder:	3
URxvt*externalBorder:	0
URxvt*depth:		32
URxvt*termName:		rxvt-256color
URxvt*iso14755:		false
URxvt*iso14755_52:	false

!URxvt*.transparent:	true
!URxvt*.shading:		110

!Tewi font
URxvt*font:             xft:lucy tewi:pixelsize=11:antialias=false
URxvt*boldFont:         xft:lucy tewi:pixelsize=11:antialias=false

!Terminal sizes
LongTerm*geometry:	80x64
WideTerm*geometry:	105x24
FillTerm*geometry:	131x64
VimTerm*geometry:	86x64
HugeTerm*geometry:	172x64

